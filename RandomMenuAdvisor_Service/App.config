<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>


  <system.serviceModel>

    <services>
      <!-- 참고: 서비스 이름은 서비스 구현에 대한 구성 이름과 일치해야 합니다. -->
      <service name="ServiceLibrary.Service.MenuAdvisor" behaviorConfiguration="MenuAdvisorServiceBehaviors" >
        <!-- 다음 끝점을 추가하십시오.  -->
        <endpoint binding="basicHttpBinding" contract="ServiceLibrary.Interface.IMenuAdvisor" />
        <!-- 참고: 서비스에 http 기본 주소가 있어야 이 끝점을 추가할 수 있습니다. -->
        <endpoint contract="IMetadataExchange" binding="mexHttpBinding" address="mex" />
      </service>
    </services>

    <behaviors>
      <serviceBehaviors>
        <behavior name="MenuAdvisorServiceBehaviors" >
          <!-- 서비스 동작 구성에 다음 요소를 추가하십시오. -->
          <serviceMetadata httpGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="true" />
        </behavior>
      </serviceBehaviors>      
    </behaviors>
    <diagnostics>
      <messageLogging logEntireMessage="true"
      maxMessagesToLog="300"
      logMessagesAtServiceLevel="true"
      logMalformedMessages="true"
      logMessagesAtTransportLevel="true" />
    </diagnostics>
  </system.serviceModel>

  <system.diagnostics>
    <sources>
      <source name="System.ServiceModel" switchValue="Verbose,ActivityTracing"
      propagateActivity="true">
        <listeners>
          <add type="System.Diagnostics.DefaultTraceListener" name="Default">
            <filter type="" />
          </add>
          <add name="xml">
            <filter type="" />
          </add>
        </listeners>
      </source>
      <source name="System.ServiceModel.MessageLogging">
        <listeners>
          <add type="System.Diagnostics.DefaultTraceListener" name="Default">
            <filter type="" />
          </add>
          <add name="xml">
            <filter type="" />
          </add>
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add initializeData="log.xml" type="System.Diagnostics.XmlWriterTraceListener"
      name="xml" traceOutputOptions="Callstack">
        <filter type="" />
      </add>
    </sharedListeners>
    <trace autoflush="true" />
  </system.diagnostics>
  
</configuration>

